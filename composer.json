{
    "name": "rivimey/spout",
    "description": "PHP Library to read and write spreadsheet files (CSV, XLSX and ODS), in a fast and scalable way",
    "type": "library",
    "keywords": ["php", "read", "write", "csv", "xlsx", "ods", "odf", "open", "office", "excel", "spreadsheet", "scale", "memory", "stream", "ooxml"],
    "license": "Apache-2.0",
    "homepage": "https://www.github.com/box/spout",
    "authors": [
        {
            "name": "Adrien Loison",
            "email": "adrien@box.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rivimey/PHPZipStreamer.git"
        }
    ],
    "require": {
        "webmozart/path-util": "~2.3",
        "rivimey/zipstreamer": "1.3",
        "php": ">=5.4.0",
        "ext-zip": "*",
        "ext-xmlreader": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.0",
        "scrutinizer/ocular": "~1.1"
    },
    "suggest": {
        "ext-iconv": "To handle non UTF-8 CSV files (if \"php-intl\" is not already installed or is too limited)",
        "ext-intl": "To handle non UTF-8 CSV files (if \"iconv\" is not already installed)"
    },
    "autoload": {
        "psr-4": {
            "Box\\Spout\\": "src/Spout"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6.x-dev"
        }
    }
}
